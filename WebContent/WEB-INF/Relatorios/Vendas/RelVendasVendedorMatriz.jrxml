	<queryString language="SQL">
		<![CDATA[select colaborador.nome as Colaborador,
 sum(pedido.valorTotalPedido) as Total, 
 emp.RazaoSocial as Empresa,
  fil.RazaoSocial as Filial,
  (select sum(p1.valorTotalPedido) from pedido p1 where p1.id_empresa = $P{empresaID}  and p1.id_filial is null and p1.pedidoStatus = "REC" and  p1.dataEmissao between  $P{dataIni}  and $P{dataFim} ) as "Total Período" from pedido 
  inner join colaborador on colaborador.id = pedido.Colaborador_id
   inner join Empresa emp on emp.id = pedido.id_empresa 
   left join Filial fil on fil.id = pedido.id_filial 
   where (pedido.dataEmissao between  $P{dataIni}  and $P{dataFim} )  and pedido.id_empresa = $P{empresaID}  and pedido.id_Filial is null  and pedido.pedidoStatus = "REC" 
   group by colaborador.nome]]>
	</queryString>
